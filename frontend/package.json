{
  "name": "react-ts-wp-scss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "cd .. && husky install",
    "check-types": "tsc",
    "lint:front": "next lint",
    "lint:front:fix": "eslint --fix --ext .js,.ts,.tsx .",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "lint:back": "cd ../backend && source .venv/bin/activate && black ./",
    "audit:back": "cd ../backend && pip-audit",
    "dev:front": "next dev",
    "dev:back": "cd ../backend && source .venv/bin/activate && uvicorn src.main:app --reload",
    "dev": "npm-run-all -p dev:front dev:back",
    "build:front": "next build",
    "serve:build": "serve -s build -p 3001",
    "build:serve": "npm run build:test && npm run serve:build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:e2e": "npm-run-all -p -r serve:build cypress:run",
    "test:local": "npm-run-all -p -r start:back:test build:serve cypress:open",
    "start:back:test": "cd ../backend && source .venv/bin/activate && ENV=test uvicorn src.main:app --reload",
    "test:back": "cd ../backend && source .venv/bin/activate && ENV=test PYTHONPATH=. pytest -v",
    "create:serve": "npm run create-users && npm run json-server",
    "build:backend": "cd ../backend && docker build -t backend . --no-cache",
    "test:back:docker": "cd ../backend && docker-compose run -e ENV=test backend bash -c \"cd /app && pytest -v\"",
    "build:test:back": "npm run build:backend && npm run test:back:docker"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "next": "^13.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "20.7.1",
    "@types/react": "18.2.23",
    "cypress": "^12.4.0",
    "cypress-localstorage-commands": "^2.2.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^13.5.3",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.0.0"
  }
}
