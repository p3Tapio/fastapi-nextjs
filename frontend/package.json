{
  "name": "react-ts-wp-scss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "cd .. && husky install",
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint:front": "next lint",
    "lint:front:fix": "eslint --fix --ext .js,.ts,.tsx .",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "lint:back": "cd ../backend && source .venv/bin/activate && black ./",
    "audit:back": "cd ../backend && pip-audit",
    "dev:front": "next dev",
    "dev:back": "cd ../backend && source .venv/bin/activate && uvicorn src.main:app --reload",
    "dev": "npm-run-all -p dev:front dev:back",
    "build:front": "next build",
    "serve:build": "serve -s build -p 3001",
    "build:serve": "npm run build:test && npm run serve:build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:e2e": "npm-run-all -p -r serve:build cypress:run",
    "test:local": "npm-run-all -p -r start:back:test build:serve cypress:open",
    "start:back:test": "cd ../backend && source .venv/bin/activate && ENV=test uvicorn src.main:app --reload",
    "test:back": "cd ../backend && source .venv/bin/activate && ENV=test PYTHONPATH=. pytest -v",
    "create:serve": "npm run create-users && npm run json-server",
    "build:backend": "cd ../backend && docker build -t backend . --no-cache",
    "test:back:docker": "cd ../backend && docker-compose run -e ENV=test backend bash -c \"cd /app && pytest -v\"",
    "build:test:back": "npm run build:backend && npm run test:back:docker"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "next": "^13.5.3",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "cypress": "^12.4.0",
    "cypress-localstorage-commands": "^2.2.4",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "serve": "^14.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
